BINDIR		=	${exec_prefix}/bin
DATADIR		=	/usr/share/cups
INCLUDEDIR	=	$(includedir)
LIBDIR		=	$(libdir)
LOCALEDIR	= 	/usr/share/locale
MANDIR		=	${prefix}/man
SBINDIR		=	${exec_prefix}/sbin
SERVERROOT	= 	/var/cups

#
# Rules...
#

.SILENT:
.SUFFIXES:	.a .c .gz .h .o .z .1 .5 .8
.c.o:
	echo Compiling $<...
	$(CC) $(CFLAGS) -c $<
.1.z .5.z .8.z .1.gz .5.gz .8.gz:
	echo Formatting $<...
	$(NROFF) -man $< >t
	$(PACK) t
	-mv t.$(CAT) $@

